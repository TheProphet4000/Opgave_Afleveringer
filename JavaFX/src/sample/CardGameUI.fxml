<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
  <children>
    <MenuBar VBox.vgrow="NEVER" />
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <ImageView fitHeight="328.0" fitWidth="258.0" layoutX="298.0" layoutY="36.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="Draw" layoutX="84.0" layoutY="252.0" mnemonicParsing="false" onAction="#drawCardButton" text="Draw card" />
            <Button fx:id="Keep" layoutX="84.0" layoutY="310.0" mnemonicParsing="false" onAction="#acceptButton" text="Keep card" textFill="#427c46" />
            <TextArea layoutX="19.0" layoutY="36.0" prefHeight="200.0" prefWidth="200.0" text="OutputText" />
      </children>
    </AnchorPane>
  </children>
</VBox>
